<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>

  <div class="col-md-8 offset-md-1">
  <table>
   <thead>
    <tr>
      <th>
      </th>
    </tr>
    </thead>
    
    <tbody>
    <% @books.each do |book| %>
      <tr>
         <td><%= link_to user_path(book.user) do %>
          <%= image_tag book.user.get_profile_image %>
         <% end %></td>
         <td><%= book.title %></td>
         <td><%= book.body %></td>
         <td><%= link_to "Show", book_path(book.id) %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
  


